<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/main.jsf</from-view-id>
    <navigation-case>
      <from-outcome>html5</from-outcome>
      <to-view-id>/simple-input-number.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>simple</from-outcome>
      <to-view-id>/simple-input.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>keyup</from-outcome>
      <to-view-id>/simple-input-instant-upper.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>thegrid</from-outcome>
      <to-view-id>/TheGrid.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>